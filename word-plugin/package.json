{
  "name": "word-ai-plugin",
  "version": "1.0.0",
  "description": "AI-powered Word plugin for context-based text completion",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "dev-server": "webpack serve --mode development",
    "start": "office-addin-debugging start manifest.xml desktop",
    "stop": "office-addin-debugging stop manifest.xml",
    "validate": "office-addin-manifest validate manifest.xml",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2"
  },
  "dependencies": {
    "@fluentui/react": "^8.119.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.7.2",
    "@types/office-js": "^1.0.382",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "office-addin-cli": "^1.5.15",
    "office-addin-debugging": "^5.0.17",
    "office-addin-dev-certs": "^1.13.2",
    "office-addin-manifest": "^1.13.2",
    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
